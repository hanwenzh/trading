using System;

namespace JY
{
    public class Capital
    {
        public Capital(string data)
        {
            string[] items = data.Split(',');
            available = items[0];
            value = items[1];
            profit = items[2];
            assets = items[3];
        }

        public string available { get; set; }
        public string value { get; set; }
        public string profit { get; set; }
        public string assets { get; set; }
    }
}